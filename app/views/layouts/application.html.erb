
<!DOCTYPE html>
<html>
<head>
  <title>Birst Dashboard</title>
  <%= stylesheet_link_tag    "application", media: "all",
    "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body class="<%= controller.controller_name %>">
  <div id="banner">
    
    <%= @page_title || "Birst Dashboard" %>
  </div>
  <div id="columns">
    <div id="side">
      <ul>
        <li><a href=dashboard>Home</a></li>
        <li><a href=ignored>Ignored From Tracking</a></li>
      </ul>
    </div>
    <div id="main">
      <%= yield %>
    </div>
  </div>
</body>
</html>